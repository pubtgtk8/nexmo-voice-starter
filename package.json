{
  "name": "nexmo-voice-starter",
  "version": "1.0.0",
  "description": "Nexmo/Vonage Voice API starter with call handling, text-to-speech, and recording",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t nexmo-voice-starter .",
    "docker:run": "docker run -e NEXMO_API_KEY=$NEXMO_API_KEY nexmo-voice-starter"
  },
  "keywords": [
    "nexmo",
    "vonage",
    "voice",
    "api",
    "tts",
    "speech",
    "telephony",
    "call",
    "recording"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@vonage/server-sdk": "^3.0.0",
    "@vonage/voice": "^2.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "multer": "^1.4.5-lts.1",
    "ws": "^8.14.0",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "joi": "^17.9.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "node-cron": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/uuid": "^9.0.0",
    "@types/multer": "^1.4.0",
    "@types/ws": "^8.4.0",
    "@types/jest": "^29.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/nexmo-voice-starter"
  }
}